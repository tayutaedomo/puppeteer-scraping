{
  "name": "corona-ex-scraping",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cex:urls": "npx ts-node scripts/coronaex_comic_urls.ts",
    "cex:details": "npx ts-node scripts/coronaex_comic_details.ts",
    "cex:concat": "python scripts/coronaex_comic_concat_details.py",
    "cex:jq": "cat tmp/coronaex_comic_details.json | jq '.[].title'",
    "cex:jq2": "cat tmp/coronaex_comic_details.json | jq '.[] | {title, likeCount}'",
    "cex:jq3": "cat tmp/coronaex_comic_details.json | jq 'map(select(.likeCount >= 50000)) | sort_by(.likeCount)'",
    "cex:jq4": "cat tmp/coronaex_comic_details.json | jq '[.[] | select(.likeCount >= 50000)] | length'",
    "cex:jq5": "cat tmp/coronaex_comic_details.json | jq -r '[.[] | select(.likeCount >= 50000)] | sort_by(.likeCount) | map([.likeCount ,.title] | join(\",\")) | join(\"\n\")'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "puppeteer": "^20.8.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@puppeteer/browsers": "^1.4.5"
  }
}
